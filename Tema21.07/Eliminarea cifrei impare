
#include <iostream>
using namespace std;

int eliminaImpare(int n) {
    int nrNou = 0, p = 1;
    while (n != 0) {
        int c = n % 10;
        if (c % 2 == 0) {  
            nrNou = nrNou + c * p;
            p *= 10;
        }
        n /= 10;
    }
    return nrNou;
}

int main() {
    int n;
    cin >> n;
    cout << "Numarul fara cifre impare este: " << eliminaImpare(n);
    return 0;
}
