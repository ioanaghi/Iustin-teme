#include <iostream>
using namespace std;

int mediaNenule(int n) {
    int suma = 0, nr = 0;
    while (n != 0) {
        int c = n % 10;
        if (c != 0) {
            suma += c;
            nr++;
        }
        n /= 10;
    }
    if (nr == 0) return 0; 
    return suma / nr;     
}

int main() {
    int n;
    cin >> n;
    cout << "Media aritmetica a cifrelor nenule este: " << mediaNenule(n);
    return 0;
}
