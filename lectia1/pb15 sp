#include <iostream>
using namespace std;

int prim(int n) {
    if (n < 2) return 0;
    for (int i = 2; i * i <= n; i++)
        if (n % i == 0)
            return 0;
    return 1;
}

int pprime(int n) {
    for (int i = n - 1; i >= 2; i--) {
        if (prim(i))
            return i;
    }
    return 0; // dacă nu există (teoretic doar pentru n<3)
}

int main() {
    cout << pprime(20);
}
